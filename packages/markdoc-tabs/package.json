    "react": "18.2.0"
  },
  "devDependencies": {
    "ctix": "2.3.0",
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "eslint": "8.55.0",
    "typescript": "5.3.3"
  },
  "eslintConfig": {
    "extends": [
      "@acme/eslint-config/base",
      "@acme/eslint-config/react"
